+	s=`cd $(srcdir); ${PWD_COMMAND}`; export s; \
+	for i in prev-*; do \
+	  j=`echo $$i | sed s/^prev-//`; \
+	  cd $$r/$$i && \
+	  { find . -type d | sort | sed 's,.*,$(SHELL) '"$$s"'/mkinstalldirs "../'$$j'/&",' | $(SHELL); } && \
+	  { find . -name '*.*da' | sed 's,.*,$(LN) -f "&" "../'$$j'/&",' | $(SHELL); }; \
+	done
