+/* Rebuild the COMPILER_PATH and LIBRARY_PATH environment variables
+   for collect.  */
+
+static void
+putenv_from_prefixes (const struct path_prefix *paths, const char *env_var,
+		      bool do_multi)
+{
+  xputenv (build_search_list (paths, env_var, true, do_multi));
+}
